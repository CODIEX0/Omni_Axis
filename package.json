{
  "name": "omni-axis-rwa-platform",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.50.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@thirdweb-dev/react-native": "^0.8.0",
    "@thirdweb-dev/sdk": "^4.0.99",
    "@types/jest": "^29.5.14",
    "ethers": "^5.8.0",
    "expo": "^53.0.11",
    "expo-camera": "~16.0.8",
    "expo-constants": "~17.0.3",
    "expo-crypto": "^14.1.5",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "^18.1.10",
    "expo-font": "~13.0.1",
    "expo-image": "^2.2.1",
    "expo-image-picker": "~16.0.2",
    "expo-linking": "~7.0.3",
    "expo-local-authentication": "~14.0.1",
    "expo-media-library": "^17.1.7",
    "expo-notifications": "~0.29.9",
    "expo-router": "~4.0.9",
    "expo-secure-store": "~14.0.0",
    "expo-status-bar": "~2.0.0",
    "expo-web-browser": "~14.0.1",
    "i18next": "^25.2.1",
    "jest-expo": "^53.0.7",
    "lucide-react-native": "^0.515.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-i18next": "^15.5.2",
    "react-native": "0.76.3",
    "react-native-chart-kit": "^6.12.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-svg": "^15.12.0",
    "react-native-toast-message": "^2.3.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-redux": "^9.2.0",
    "recharts": "^2.15.3",
    "redux-persist": "^6.0.0",
    "victory-native": "^41.17.4",
    "zod": "^3.25.57",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^24.0.1",
    "@types/react": "~18.3.12",
    "@types/react-native": "~0.73.0",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.3.3"
  },
  "expo": {
    "name": "Omni Axis",
    "slug": "omni-axis-rwa-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "omniaxis",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.omniaxis.rwa",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture documents for KYC verification and asset documentation.",
        "NSPhotoLibraryUsageDescription": "This app accesses your photo library to upload asset images and documents.",
        "NSFaceIDUsageDescription": "Use Face ID to securely access your account and authorize transactions.",
        "NSLocationWhenInUseUsageDescription": "Location is used for compliance and fraud prevention."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1E40AF"
      },
      "package": "com.omniaxis.rwa",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-camera",
      "expo-document-picker",
      "expo-image-picker",
      "expo-local-authentication",
      "expo-notifications",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "omni-axis-rwa-platform"
      }
    }
  }
}
